<div class="container"
<p>Find me in app/views/topics/new.html.erb</p>
</div>
<div class="container">

<h3>Enter Category for your Blocmarks</h3>
<div class="row">
  <div class="col-md-8">
    <%= form_for @topic do |f| %>
      <div class="form-group">
        <%= f.text_field :title, class: 'form-control', placeholder: "Enter Category" %>
      </div>
       <%= f.submit "Save", class: 'btn btn-primary' %>
    <% end %>
  </div>
</div>
</div>
